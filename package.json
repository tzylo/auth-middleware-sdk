{
  "name": "@tzylo/auth-middleware",
  "version": "1.1.0",
  "description": "Tzylo Auth CE middleware for Fastify and Express",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./fastify": {
      "types": "./dist/fastify.d.ts",
      "import": "./dist/fastify.js",
      "require": "./dist/fastify.cjs"
    },
    "./express": {
      "types": "./dist/express.d.ts",
      "import": "./dist/express.js",
      "require": "./dist/express.cjs"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --minify",
    "dev": "tsup src/index.ts --watch --format cjs,esm",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "auth",
    "authentication",
    "jwt",
    "fastify",
    "express",
    "middleware",
    "tzylo"
  ],
  "author": "K B Pramod",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tzylo/auth-middleware-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/tzylo/auth-middleware-sdk/issues"
  },
  "peerDependencies": {
    "fastify": "^5",
    "express": "^4 || ^5"
  },

  "dependencies": {
    "jsonwebtoken": "^9.0.3"
  },

  "devDependencies": {
    "tsup": "^8.5.1",
    "typescript": "^5.4.0",
    "@types/node": "^20.0.0",
    "@types/express": "^4 || ^5",
    "@types/jsonwebtoken": "^9.0.10"
  }
}
